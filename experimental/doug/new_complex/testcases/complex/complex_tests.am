tests32_cabsf_CPPFLAGS = $(CPPFLAGS32) -DTESTFUNC=cabs -D_NEW_COMPLEX_FLOAT $(AM_CPPFLAGS)
tests32_cabsf_LDFLAGS = $(LDFLAGS32) $(AM_LDFLAGS)
